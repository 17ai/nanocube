{
    "nanocube":{
        "NoD": {"url": "http://localhost:12345"}
    },
    
    "data":{
        "egress_tp": {
            "expr": "NoD[type=='EGRESS_TP']",
            "colormap": "Reds"
        },
        "ingress_tp": {
            "expr": "NoD[type=='INGRESS_TP']",
            "colormap": "Blues"
        },
        "bw": {
            "expr": "NoD[type=='SPOT_BW']",
            "colormap": "Greens",
            "disabled": true
        }
    },
    
    "widget":{
        "location":{
            "type": "spatial",
            "div":{
                "title": "location",
                "float":"left",
                "width": "100%",
                "height": "100%",
                "z-index": 0,
                "coarse_offset":0,
                "tilesurl": "http://{s}.tile.stamen.com/toner-lite/{z}/{x}/{y}.png",
                "viewbox": [[21.8,-126.2],[51.9,-62.6]]
            }
        },

        "cust_name":{
            "type": "cat",
            "div":{
                "title": "customer name",
                "opacity": 0.8, 
                "top": "5em", 
                "height": "30em", 
                "width": "45em",
                "margin-left": "42ch",
                "z-index": 1, 
                "position": "absolute", 
                "background-color": "#555", 
                "right": "1ch",
                "logaxis": "true",
                "overflow": "auto"
            }
        },

        "utc":{
            "type": "time",
            "div":{
                "title": "time",
                "opacity": 0.8, 
                "bottom": "1em", 
                "height": "12em", 
                "width": "70%", 
                "z-index": 1, 
                "position": "absolute", 
                "background-color": "#555", 
                "left": "1ch"
            }
        }
    }
}
